{
  "name": "clean-express-prisma",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --quiet --transpile-only --files app.ts",
    "start": "NODE_ENV=production node dist/app.js",
    "build": "tsc && tsc-alias",
    "postinstall": "tsc && tsc-alias",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma generate && prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^2.29.0",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-file-routing": "^1.1.3",
    "helmet": "^4.6.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/node": "15.14.1",
    "prettier": "^2.3.2",
    "prisma": "^2.29.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.3.9",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
